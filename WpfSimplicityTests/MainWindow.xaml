<Window x:Class="WpfSimplicityTests.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfSimplicityTests"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="39*"/>
            <ColumnDefinition Width="761*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Grid.Column="1" Content="Введите проверяемое число:" HorizontalAlignment="Left" Margin="69,59,0,0" VerticalAlignment="Top" Width="229"/>
        <Button x:Name="button" Grid.Column="1" Content="Проверить" HorizontalAlignment="Left" Margin="436,62,0,0" VerticalAlignment="Top" Click="button_Click"/>
        <Label x:Name="label1" Visibility="Hidden" Grid.Column="1" Content="Тест Соловея-Штрассена" HorizontalAlignment="Left" Margin="102,0,0,0" VerticalAlignment="Center" RenderTransformOrigin="1.035,0.565" IsEnabled="False"/>
        <Label x:Name="label2" Visibility="Hidden" Grid.Column="1" Content="Label" HorizontalAlignment="Left" Margin="302,0,0,0" VerticalAlignment="Center" RenderTransformOrigin="-0.026,0.412" IsEnabled="False"/>
        <Label x:Name="label3" Visibility="Hidden" Grid.Column="1" Content="Тест Миллера-Рабина" HorizontalAlignment="Left" Margin="102,253,0,0" VerticalAlignment="Top" IsEnabled="False"/>
        <Label x:Name="label4" Visibility="Hidden" Grid.Column="1" Content="Label" HorizontalAlignment="Left" Margin="302,253,0,0" VerticalAlignment="Top" IsEnabled="False"/>
        <Label x:Name="label5" Visibility="Hidden" Grid.Column="1" Content="Вероятностный алгоритм" HorizontalAlignment="Left" Margin="102,304,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.877,0.488" IsEnabled="False"/>
        <Label x:Name="label6" Visibility="Hidden" Grid.Column="1" Content="Label" HorizontalAlignment="Left" Margin="302,304,0,0" VerticalAlignment="Top" RenderTransformOrigin="-2.72,2.133" IsEnabled="False"/>
        <TextBox x:Name="textBox" Grid.Column="1" HorizontalAlignment="Left" Margin="261,63,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="Final_Label"  Visibility="Hidden" Grid.Column="1" Content="Label" HorizontalAlignment="Left" Margin="121,130,0,0" VerticalAlignment="Top" IsEnabled="False"/>
        <Label x:Name="label7" Grid.Column="1" Content="Время работы алгоритма" HorizontalAlignment="Center" Margin="0,143,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
